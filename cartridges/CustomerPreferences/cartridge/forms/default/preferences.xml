<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19" secure="false">
	<group formid="customerPreferences">
		<!-- Birhday -->
		<field
			formid="birthday"
			label="label.profile.birthday"
			mandatory="false"
			missing-error="error.message.required"
			range-error="error.message.50orless"
			type="date" />

		<!-- Interests -->
		<!-- Electronic -->
		<field
			formid="interests-electronic"
			label="label.profile.interests"
			mandatory="false"
			missing-error="error.message.required"
			range-error="error.message.50orless"
			type="boolean" />
		<!-- Apparel -->
		<field
			formid="interests-apparel"
			label="label.profile.interests"
			mandatory="false"
			missing-error="error.message.required"
			range-error="error.message.50orless"
			type="boolean" />

		<!-- Newsletter -->
		<field
			formid="newsletterSubscription"
			label="label.profile.newsletter"
			mandatory="false"
			missing-error="error.message.required"
			parse-error="error.message.parse.email.profile.form"
			range-error="error.message.50orless"
			type="boolean"/>
	</group>

</form>
